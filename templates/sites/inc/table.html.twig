<table>
    <thead>
        <tr>
            <th>id</th>
            <th>datum</th>
            <th>partia</th>
            <th>cvik</th>
            <th>vaha</th>
            <th>prvaSeria</th>
            <th>ciel</th>
            <th>realizovane</th>
            <th>poznaky</th>
        </tr>
    </thead> 
    <tbody>
        {% for item in data %}
            <tr id="{{ item.id }}">

                <td>{{ item.id}}</td>
                <td>{{ item.datum|date('d-m-Y')}}</td>
                <td>{{ item.partia}}</td>
                <td>{{ item.cvik}}</td>
                <td>{{ item.vaha}}</td>
                <td>{{ item.prvaSeria}}</td>
                <td>{{ item.ciel}}</td>
                <td>{{ item.realizovane}}</td>
                <td>{{ item.poznamky}}</td>

            </tr>
        {% endfor %}    
    </tbody>
</table>

